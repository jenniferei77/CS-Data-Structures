#use <conio>

bool test_prog(string input, int output) {
  queue_t Q = tokenize(input);
  istack_t S = istack_new();
  eval(S, Q);
  println("in here");
  if (istack_empty(S)) return false;
  int i = ipop(S);
  if (!istack_empty(S)) return false;
  println("can i print");
  printint(i); print("\n");
  return i == output;
}

int main() {
  //assert(test_prog("3 4 * -9 2 / +", 8));
  // Add more tests of your own!
  test_prog("3 4 * -9 2 / +", 8);
  
  println("can i print");

  println("All tests passed!");
  return 0;
}
